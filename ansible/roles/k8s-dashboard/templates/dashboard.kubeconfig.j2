apiVersion: v1
kind: Config
current-context: kubelet-to-example.com
preferences: {}
clusters:
- cluster:
    certificate-authority: /etc/kubernetes/certs/ca.crt
    server: https://{{ ansible_host }}:6443
  name: example.com
contexts:
- context:
    cluster: example.com
    user: dashboard
  name: kubelet-to-example.com
users:
- name: dashboard
  user:
    token: {{ dashboard_token }}
